{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "cutie",
  "version": "0.1.0",
  "identifier": "com.cutie.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1421",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "cutie",
        "width": 1580,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "transparent": true,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
